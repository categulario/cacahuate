<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="https://tracsa.github.io/vi-xml/proceso_transform.xsl" ?>
<process-spec>
  <process-info>
    <author>OgAstorga</author>
    <date>2019-01-18</date>
    <name>Datetime input</name>
    <description>Process to test the datetime input</description>
    <public>false</public>
  </process-info>
  <process>
    <action id="link_node" >
      <node-info>
        <name>Unnamed action</name>
        <description>Undescribed action</description>
      </node-info>
      <auth-filter backend="anyone"></auth-filter>
      <form-array>
        <form id="auth_form">
          <input
            type="datetime"
            label="Give me the datetime"
            name="datetime" />
        </form>
      </form-array>
    </action>
  </process>
</process-spec>
