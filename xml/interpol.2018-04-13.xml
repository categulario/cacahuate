<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="https://tracsa.github.io/vi-xml/proceso_transform.xsl" ?>
<process-spec>
  <process-info>
    <author>categulario</author>
    <date>2018-04-13</date>
    <name>Computes a name based on a {{ form.field }}</name>
    <public>false</public>
    <description>Uses string interpolation to build the execution's name</description>
  </process-info>
  <process>
    <action id="start_node" >
      <node-info>
        <name>Unnamed action</name>
        <description>Undescribed action</description>
      </node-info>
      <auth-filter backend="anyone"></auth-filter>
      <form-array>
        <form id="form">
          <input type="text" name="field" required="required" label="Label-less form"/>
        </form>
      </form-array>
    </action>
  </process>
</process-spec>
