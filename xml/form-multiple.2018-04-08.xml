<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="https://tracsa.github.io/vi-xml/proceso_transform.xsl" ?>
<process-spec>
  <process-info>
    <author>categulario</author>
    <date>2018-04-08</date>
    <name>Con un formulario m√∫ltiple</name>
    <description>Este proceso tiene un formulario que puede enviar muchas copias</description>
    <public>true</public>
  </process-info>
  <process>
    <action id="form_node" >
      <node-info>
        <name>Unnamed action</name>
        <description>Undescribed action</description>
      </node-info>
      <auth-filter backend="anyone"></auth-filter>
      <form-array>
        <form id="single_form">
          <input type="text" name="name" required="required" label="Single Form"/>
        </form>
        <form id="multiple_form" multiple="1-10">
          <input type="text" name="phone" required="required" label="Multi Form"/>
        </form>
      </form-array>
    </action>
  </process>
</process-spec>
